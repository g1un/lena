.title
	text-align center
	position relative
	margin-bottom 35px
	max-width 100%
	overflow hidden
	h2
		display inline-block
		vertical-align top
		padding 0 25px
		font-ptserif-bold()
		font-size 24px
		line-height 1.2em
		color #010101
		text-transform uppercase
		background #fff
		position relative
		&:before,
		&:after
			content ''
			position absolute
			top 0
			bottom 0
			width 100px
			background #fff
		&:before
			top 50%
			right 100%
			width 9999px
			height 1px
			background linear-gradient(to left, transparent, #d9dbdd 100px)
		&:after
			top 50%
			left 100%
			width 9999px
			height 1px
			background linear-gradient(to right, transparent, #d9dbdd 100px)